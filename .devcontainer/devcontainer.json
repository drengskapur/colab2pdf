{
    "name": "Colab Local Runtime",
    "dockerFile": "../Dockerfile",
    "remoteUser": "root",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash",
                "extensions": [
                    "ms-python.python"
                ]
            }
        }
    },
    "forwardPorts": [
        9000
    ],
    "postCreateCommand": "pip install jupyter_http_over_ws==0.0.8 ipykernel"
}