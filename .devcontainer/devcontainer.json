{
    "name": "Google Colab",
    "dockerFile": "Dockerfile",
    "context": "..",
    "remoteUser": "root",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash",
                "extensions": [
                    "ms-python.python",
                    "ms-toolsai.jupyter"
                ]
            }
        }
    },
    "forwardPorts": [
        9000
    ],
    "postCreateCommand": "pip install jupyter_http_over_ws==0.0.8 ipykernel"
}